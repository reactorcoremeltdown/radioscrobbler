<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" 
    "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>space.rcmd.radioscrobbler</string>

    <key>ProgramArguments</key>
    <array>
        <string>/opt/homebrew/bin/radioscrobbler</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/tmp/radioscrobbler.log</string>

    <key>StandardErrorPath</key>
    <string>/tmp/radioscrobbler.err.log</string>
</dict>
</plist>
